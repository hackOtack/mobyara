<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <ripple android:color="@color/levelup_navigation_bar_bg_ripple">
            <item android:drawable="@drawable/navigation_bar_item_bg_selected" />
        </ripple>
    </item>
    <item>
        <ripple android:color="@color/levelup_navigation_bar_bg_ripple">
            <item android:drawable="@drawable/navigation_bar_item_bg_unselected" />
        </ripple>
    </item>
</selector>