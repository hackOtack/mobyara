<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item state_checked="true">
        <shape shape="rectangle">
            <solid color="#ffffffff" />
            <corners topRightRadius="4.0dip" bottomRightRadius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape shape="rectangle">
            <solid color="#00000000" />
            <stroke width="1.0dip" color="#ffffffff" />
            <corners topRightRadius="4.0dip" bottomRightRadius="4.0dip" />
        </shape>
    </item>
</selector>